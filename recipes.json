{
    "recipes": {
        "aom": {
            "quality_start": 4,
            "quality_end": 63,
            "quality_step": 1,
            "encode_extension": "avif",
            "decode_extension": "png",
            "encode_cmd": "avifenc --tilecolslog2 1 -c aom -s d --min $quality --max $quality -o $target $origpng",
            "lossless_cmd": "avifenc --lossless --tilecolslog2 1 -c aom -s d -o $target $origpng",
            "decode_cmd": "avifdec -c dav1d $target $target_dec",
            "export_to_png": true
        }, 
        "bpg": {
            "quality_start": 1,
            "quality_end": 51,
            "quality_step": 1,
            "encode_extension": "bpg",
            "decode_extension": "png",
            "encode_cmd": "bpgenc -m 8 -f 420 -q $quality -o $target $origpng",
            "lossless_cmd": "bpgenc -m 8 -f 420 -lossless -o $target $origpng",
            "decode_cmd": "bpgdec -o $target_dec $target",
            "export_to_png": true
        },
        "jxl": {
            "quality_start": 8,
            "quality_end": 100,
            "quality_step": 2,
            "encode_extension": "jxl",
            "decode_extension": "png",
            "encode_cmd": "cjxl $origpng $target -v -q $quality",
            "lossless_cmd": "cjxl $origpng $target -v -q 100",
            "decode_cmd": "djxl $target $target_dec",
            "export_to_png": true
        },
        "mozjpeg": {
            "quality_start": 5,
            "quality_end": 95,
            "quality_step": 1,
            "encode_extension": "jpg",
            "decode_extension": "ppm",
            "encode_cmd": "cjpeg -quality $quality -outfile $target $origpng",
            "lossless_cmd": "cjpeg -rgb -quality 100 -outfile $target $origpng",
            "decode_cmd": "djpeg -outfile $target_dec $target",
            "export_to_png": true
        },
        "rav1e": {
            "quality_start": 1,
            "quality_end": 201,
            "quality_step": 5,
            "encode_extension": "avif",
            "decode_extension": "png",
            "encode_cmd": "avifenc --tilecolslog2 1 -c rav1e -s d --min $quality --max $quality -o $target $origpng",
            "lossless_cmd": "avifenc --lossless --tilecolslog2 1 -c rav1e -s d -o $target $origpng",
            "decode_cmd": "avifdec -c dav1d $target $target_dec",
            "export_to_png": true
        },
        "svt-av1": {
            "quality_start": 4,
            "quality_end": 63,
            "quality_step": 1,
            "encode_extension": "avif",
            "decode_extension": "png",
            "encode_cmd": "avifenc --tilecolslog2 1 -c svt -s d -y 420 --min $quality --max $quality -o $target $origpng",
            "lossless_cmd": "avifenc --lossless --tilecolslog2 1 -c svt -s d -y 420 -o $target $origpng",
            "decode_cmd": "avifdec -c dav1d $target $target_dec",
            "export_to_png": true
        },
        "webp": {
            "quality_start": 0,
            "quality_end": 95,
            "quality_step": 1,
            "encode_extension": "webp",
            "decode_extension": "png",
            "encode_cmd": "cwebp -mt -q $quality -o $target $origpng",
            "lossless_cmd": "cwebp -mt -z 9 -lossless -o $target $origpng",
            "decode_cmd": "dwebp -o $target_dec $target",
            "export_to_png": true
        },
        "webp2": {
            "quality_start": 0,
            "quality_end": 95,
            "quality_step": 1,
            "encode_extension": "wp2",
            "decode_extension": "png",
            "encode_cmd": "cwp2 -mt 2 -q $quality -o $target $origpng",
            "lossless_cmd": "cwp2 -mt 2 -q 100 -o $target $origpng",
            "decode_cmd": "dwp2 -o $target_dec $target",
            "export_to_png": true
        }
    }
}
